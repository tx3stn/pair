#!/usr/bin/expect -f

set env(ACCESSIBLE) true
set timeout 10
spawn pair commit
match_max 100000
expect -re ".*enter ticket ID\:(.|\n|\r)*"
send -- "PROMPT-123\r"
expect -re ".*paring with\:(.|\n|\r)*"
send -- "1\r"
expect -re ".*select commit type\:(.|\n|\r)*"
send -- "1\r"
expect -re ".*commit message\:(.|\n|\r)*"
send -- "test commit message\r"
expect eof
